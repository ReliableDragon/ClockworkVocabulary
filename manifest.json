{
    "name": "ClockworkVocabulary",
    "version": "1.0",
    "description": "Build your vocabulary!",
    "permissions": [
      "storage",
      "https://developer.chrome.com/"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/CV16.png"
      }
    },
    "icons": {
      "16": "images/CV16.png"
    },
    "manifest_version": 2
}
